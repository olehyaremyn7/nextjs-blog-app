  "engines": {
    "node": ">=20.x",
    "npm": ">=9.x"
  },
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "prisma:dev": "prisma studio",
    "lint": "next lint",
    "lint:fix": "yarn lint --fix",
    "lint:hook": "yarn lint:fix --no-cache && yarn lint --quiet --no-cache",
    "format": "prettier \"./**/*.{js,jsx,css,md,json}\"",
    "format:fix": "yarn format --write --ignore-unknown --cache",
    "format:hook": "yarn format:fix && yarn format --ignore-unknown --list-different",
    "prepare": "husky install"
  },
  "dependencies": {
    "@auth/prisma-adapter": "^1.0.2",
    "@prisma/client": "^5.3.1",
    "clsx": "^2.0.0",
    "dayjs": "^1.11.10",
    "firebase": "^10.4.0",
    "isomorphic-dompurify": "^2.0.0",
    "next": "13.4.19",
    "next-auth": "^4.23.1",
    "prop-types": "^15.8.1",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-hook-form": "^7.47.0",
    "react-quill": "^2.0.0"
  },
  "devDependencies": {
    "eslint": "8.48.0",
    "eslint-config-next": "13.4.19",
    "eslint-config-prettier": "^9.0.0",
    "husky": "^8.0.3",
    "prettier": "^3.0.3",
    "prisma": "^5.3.1"
  },
  "repository": {
    "type": "git",
