  "keywords": [
    "Next.js",
    "Next Auth",
    "React.js",
    "Prisma",
    "MongoDB",
    "Firebase",
    "React Hook Form",
    "React Quill",
    "Auth",
    "dayjs",
    "eslint",
    "prettier",
    "husky",
    "yarn"
  ]
